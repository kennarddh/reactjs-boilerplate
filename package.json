{
	"name": "reactjs-boilerplate",
	"version": "1.0.0",
	"description": "React js boilerplate",
	"main": "src/index.js",
	"type": "module",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint:build": "tsc --project tsconfig.eslint.json",
		"lint:check": "eslint --cache ./**/*.{ts,tsx,json} && tsc --noemit",
		"lint:fix": "eslint --cache --fix ./**/*.{ts,tsx,json} && tsc --noemit",
		"prettier:fix": "prettier -w \"./**/*.{html,ts,tsx,json,md,prettierrc}\"",
		"prettier:check": "prettier -c \"./**/*.{html,ts,tsx,json,md,prettierrc}\"",
		"clean": "rimraf build",
		"lint:staged": "lint-staged",
		"prepare": "npm run lint:build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kennarddh/reactjs-boilerplate.git"
	},
	"author": "kennarddh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kennarddh/reactjs-boilerplate/issues"
	},
	"homepage": "https://github.com/kennarddh/reactjs-boilerplate#readme",
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/eslint": "^8.56.9",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.78",
		"@types/react-dom": "^18.2.25",
		"@typescript-eslint/utils": "^7.6.0",
		"@vitejs/plugin-react": "^4.2.1",
		"eslint": "^8.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-security": "^3.0.0",
		"globals": "^15.0.0",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.6.0",
		"vite": "^5.2.8",
		"vite-plugin-checker": "^0.6.4",
		"vite-plugin-svgr": "^4.2.0"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^6.1.8"
	}
}
