{
	"name": "reactjs-boilerplate",
	"version": "1.0.0",
	"description": "React js boilerplate",
	"main": "src/index.js",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint:check": "eslint src/**/*.{ts,tsx,json} {jest.config,jest.setup}.ts",
		"lint:fix": "eslint --fix src/**/*.{ts,tsx,json} {jest.config,jest.setup}.ts",
		"prettier:fix": "prettier -w \"src/**/*.{ts,tsx,json,md}\" \"*.md\" \"{jest.config,jest.setup}.ts\" \"index.html\"",
		"prettier:check": "prettier -c \"src/**/*.{ts,tsx,json,md}\" \"*.md\" \"{jest.config,jest.setup}.ts\" \"index.html\"",
		"clean": "rimraf build",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch --passWithNoTests",
		"test:watch:all": "jest --watchAll --passWithNoTests",
		"test:coverage": "jest --coverage --passWithNoTests",
		"test:changed": "jest -o --passWithNoTests",
		"pre-commit": "lint-staged --quiet",
		"pre-push": "npm run test",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kennarddh/reactjs-boilerplate.git"
	},
	"keywords": [
		"react",
		"reactjs",
		"boilerplate",
		"vite"
	],
	"author": "kennarddh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kennarddh/reactjs-boilerplate/issues"
	},
	"homepage": "https://github.com/kennarddh/reactjs-boilerplate#readme",
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-react": "^7.22.5",
		"@babel/preset-typescript": "^7.22.5",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/jest": "^29.5.2",
		"@types/react": "^18.2.9",
		"@types/react-dom": "^18.2.4",
		"@types/styled-components": "^5.1.26",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@vitejs/plugin-react": "^4.0.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-plugin-styled-components": "^2.1.3",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-security": "^1.7.1",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-styled-components": "^7.1.1",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-checker": "^0.6.0",
		"vite-plugin-svgr": "^3.2.0"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^5.3.11"
	}
}
