{
	"name": "reactjs-boilerplate",
	"version": "1.0.0",
	"description": "React js boilerplate",
	"main": "src/index.js",
	"scripts": {
		"start": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint:check": "eslint src/**/*.{js,jsx,json}",
		"lint:fix": "eslint --fix src/**/*.{js,jsx,json}",
		"prettier:fix": "prettier -w \"src/**/*.{js,jsx,json,md}\" \"*.md\"",
		"prettier:check": "prettier -c \"src/**/*.{js,jsx,json,md}\" \"*.md\"",
		"clean": "rimraf build",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch --passWithNoTests",
		"test:watch:all": "jest --watchAll --passWithNoTests",
		"test:coverage": "jest --coverage --passWithNoTests",
		"test:changed": "jest -o --passWithNoTests",
		"pre-commit": "lint-staged",
		"pre-push": "npm run test",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kennarddh/reactjs-boilerplate.git"
	},
	"keywords": [
		"react",
		"reactjs",
		"boilerplate",
		"vite"
	],
	"author": "kennarddh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kennarddh/reactjs-boilerplate/issues"
	},
	"homepage": "https://github.com/kennarddh/reactjs-boilerplate#readme",
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-react": "^7.18.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.4.3",
		"@vitejs/plugin-react": "^2.1.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-styled-components": "^2.0.7",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.0.4",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-security": "^1.5.0",
		"husky": "^8.0.1",
		"jest": "^29.0.1",
		"jest-environment-jsdom": "^28.1.3",
		"jest-styled-components": "^7.1.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"vite": "^3.1.6",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-svgr": "^2.2.1"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"styled-components": "^5.3.5"
	}
}
